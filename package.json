{
  "name": "nestjs-module",
  "version": "1.0.2",
  "private": true,
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "prettier": "prettier --check 'packages/**/{src,test}/**/*.ts'",
    "clean": "pnpm recursive exec -- rm -rf node_modules;  rm -rf node_modules",
    "build": "pnpm recursive run build",
    "prebuild": "pnpm recursive run prebuild",
    "publish": "pnpm recursive run publish"
  },
  "devDependencies": {
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.3.2"
  }
}
